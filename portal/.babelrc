{
  "presets": [
    [
      "@babel/env",
      {
        "modules": false,
        "corejs": 3,
        "targets": {
          "browsers": ["> 0.25%, not dead"]
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-transform-regenerator",
    [
      "@babel/plugin-transform-runtime",
      {
        "helpers": true,
        "regenerator": true
      }
    ]
  ]
}
